              td.half(align='center' style="font-family: 'Mulish', sans-serif; font-size: 14px; vertical-align: top; width: 33%; text-align: center; display: inline-block;" width='33%' valign='top')
                a.social-icons(href='https://www.facebook.com/unikaksha/' style='color: #ec0867; text-decoration: underline; box-sizing: border-box; border: 2px rgb(43, 39, 63, 0.1) solid; margin: 0 3px; width: 40px; height: 40px; border-radius: 100%; display: inline-block; padding: 7px;')
