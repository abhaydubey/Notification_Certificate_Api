                a.social-icons(href='https://www.linkedin.com/company/unikaksha' style='color: #ec0867; text-decoration: underline; box-sizing: border-box; border: 2px rgb(43, 39, 63, 0.1) solid; margin: 0 3px; width: 40px; height: 40px; border-radius: 100%; display: inline-block; padding: 7px;')
                  img.linkedin-icon(src='https://kaksha-docs.s3.ap-south-1.amazonaws.com/images/linkedin.png' style='border: none; -ms-interpolation-mode: bicubic; max-height: 20px; max-width: 100%;')
              td.half(align='center' style="font-family: 'Mulish', sans-serif; font-size: 14px; vertical-align: top; width: 33%; text-align: center; display: inline-block;" width='33%' valign='top')
                a.social-icons(href='https://www.youtube.com/channel/UCp5Id5P-KDLUMl8AVb13bFg' style='color: #ec0867; text-decoration: underline; box-sizing: border-box; border: 2px rgb(43, 39, 63, 0.1) solid; margin: 0 3px; width: 40px; height: 40px; border-radius: 100%; display: inline-block; padding: 7px;')
                  img.youtube-icon(src='https://kaksha-docs.s3.ap-south-1.amazonaws.com/images/youtube.png' style='border: none; -ms-interpolation-mode: bicubic; max-height: 20px; max-width: 100%;')
        // Footer end
        // END CENTERED WHITE CONTAINER
    td(style="font-family: 'Mulish', sans-serif; font-size: 14px; vertical-align: top;" valign='top') &nbsp;
